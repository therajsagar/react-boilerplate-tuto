        ],
        "useBuiltIns": "entry",
        "corejs": "2.6.10"
    }], "@babel/preset-react"],
    "plugins": ["react-hot-loader/babel", "@babel/plugin-syntax-dynamic-import"],
    "env": {
        "test": {
            "plugins": ["dynamic-import-node"]
        }
    }
